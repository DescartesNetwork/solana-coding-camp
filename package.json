    "swiper": "^8.3.2",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "nodemon --watch * --exec \"REACT_APP_ENV=development PORT=8000 craco start\"",
    "build": "REACT_APP_ENV=production craco build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "impImg": "node ./import-builder.js",
