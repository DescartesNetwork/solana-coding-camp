    "@types/react-router-dom": "^5.3.2",
    "antd": "^4.16.13",
    "nodemon": "^2.0.14",
    "numbro": "^2.3.6",
    "rc-animate": "^3.1.1",
    "rc-tween-one": "^2.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
