    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "dotenv-cra": "^3.0.2",
    "gh-pages": "^4.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "postcss": "^8.3.11",
    "postcss-prefixwrap": "^1.25.0",
    "prettier": "^2.6.2"
